target :MarvelAPIClient do
	pod 'BothamNetworking', :git => 'git@github.com:Karumi/BothamNetworking.git'
	pod 'CryptoSwift'
	pod 'SwiftyJSON'
end

target :MarvelAPIClientTests do
	pod 'BothamNetworking', :git => 'git@github.com:Karumi/BothamNetworking.git'
	pod 'CryptoSwift'
	pod 'SwiftyJSON'
    pod 'Nocilla'
    pod 'Nimble'
end